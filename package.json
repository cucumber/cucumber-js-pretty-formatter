{
  "name": "cucumber-pretty",
  "version": "1.5.2",
  "description": "Cucumber.js Pretty Formatter",
  "repository": "https://github.com/kozhevnikov/cucumber-pretty",
  "author": "Ilya Kozhevnikov <git@kozhevnikov.com>",
  "license": "MIT",
  "main": "index.js",
  "files": [
    "index.js"
  ],
  "scripts": {
    "lint": "eslint .",
    "test": "mocha"
  },
  "dependencies": {
    "cli-table3": "^0.5.1",
    "colors": "^1.3.3",
    "figures": "^3.0.0"
  },
  "peerDependencies": {
    "cucumber": ">=3.0.0"
  },
  "devDependencies": {
    "cucumber": "^5.1.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.17.3",
    "mocha": "^6.1.4",
    "should": "^13.2.3"
  },
  "eslintConfig": {
    "extends": "airbnb-base",
    "rules": {
      "comma-dangle": [
        "error",
        "never"
      ],
      "func-names": "off",
      "function-paren-newline": "off",
      "max-len": "off",
      "no-console": "off",
      "object-curly-newline": "off",
      "object-property-newline": "off",
      "operator-linebreak": "off"
    }
  }
}
